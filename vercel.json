{
  "version": 2,
  "builds": [
    {
      "src": "chat/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "chat/app.py"
    }
  ],
  "env": {
    "GROQ_API_KEY": "@groq_api_key"
  }
}
